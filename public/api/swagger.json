    "definitions": {
        "Error": {
            "required": [
                "code",
                "message"
            ],
            "properties": {
                "code": {
                    "type": "integer",
                    "format": "int32"
                },
                "message": {
                    "type": "string"
                }
            }
        },
        "Extra": {
            "required": [],
            "properties": {
                "from": {
                    "title": "from",
                    "type": "Point"
                },
                "to": {
                    "title": "to",
                    "type": "Point"
                }
            }
        },
        "Point": {
            "required": [],
            "properties": {
                "lat": {
                    "title": "lat",
                    "type": "float",
                    "example": "37.3243546"
                },
                "lon": {
                    "title": "lon",
                    "type": "float",
                    "example": "51.2345634"
                }
            }
        },
        "OrderItem": {
            "required": [
                "price",
                "itemType",
                "itemIdentifier",
                "extra"
            ],
            "properties": {
                "identifier": {
                    "title": "identifier",
                    "description": "UUID",
                    "type": "string",
                    "example": "24rf2rf3"
                },
                "itemType": {
                    "title": "itemType",
                    "type": "string",
                    "example": "peik"
                },
                "itemIdentifier": {
                    "title": "itemIdentifier",
                    "type": "string",
                    "example": "2"
                },
                "extra": {
                    "title": "extra",
                    "type": "Extra",
                    "example": "{}"
                }
            }
        },
        "Order": {
            "required": [
                "status"
            ],
            "properties": {
                "identifier": {
                    "title": "identifier",
                    "description": "UUID",
                    "type": "string",
                    "example": "42t34gt34t"
                },
                "status": {
                    "title": "status",
                    "type": "string",
                    "example": "0"
                },
                "data": {
                    "title": "date",
                    "type": "string",
                    "example": "23456723"
                },
                "items": {
                    "title": "items",
                    "type": "array",
                    "items": {
                        "$ref": "OrderItem"
                    }
                }
            }
        }
    }
